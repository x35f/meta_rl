{
  "env_name": "HalfCheetah-v2",
  "buffer":{
    "per": true, 
    "max_buffer_size": 1000000, 
    "alpha": 0.6, 
    "init_beta": 0.4, 
    "final_beta": 1, 
    "beta_decay": 1e-5, 
    "epsilon": 0.001
  },
  "agent":{
    "per": true, 
    "gamma": 0.99,
    "update_target_network_interval": 1,
    "target_smoothing_tau": 0.005,
    "alpha": 0.2,
    "q_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "learning_rate":0.001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256,256],
      "optimizer_class": "Adam",
      "deterministic": false,
      "learning_rate":0.001,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "re_parameterize": true
    },
    "entropy":{
      "automatic_tuning": true,
      "learning_rate": 0.001,
      "optimizer_class": "Adam"
    }
  },
  "trainer":{
    "max_iteration": 60000,
    "num_updates_per_iteration":50,
    "num_steps_per_iteration": 50,
    "update_n_interval": 50,
    "batch_size": 100,
    "max_trajectory_length":1000,
    "test_interval": 20,
    "num_test_trajectories": 5,
    "save_model_interval": 2000,
    "start_timestep": 1000,
    "save_video_demo_interval": 1000,
    "log_interval": 50
  },
  "env":{
    "reward_scale": 10.0
  }
  
}
